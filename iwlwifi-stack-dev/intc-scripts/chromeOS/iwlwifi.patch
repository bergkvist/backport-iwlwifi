+	if (iwl_mvm_is_lar_supported(mvm))
+		rd = iwl_mvm_get_current_regdomain(mvm, NULL);
+	else
+		rd = NULL;
+	mutex_unlock(&mvm->mutex);
+
+	if (IS_ERR_OR_NULL(rd))
